{
  "name": "ng-workspace",
  "version": "0.0.0",
  "scripts": {
    "start": "yarn serve & yarn watch",
    "build": "yarn learner:build --prod & yarn admin:build --prod & yarn marvel:build --prod",
    "server": "ts-node --project tsconfig.json server.ts",
    "learner:build": "ng build --project learner",
    "learner:watch": "yarn learner:build --watch",
    "admin:build": "ng build --project admin",
    "admin:watch": "yarn admin:build --watch",
    "marvel:build": "yarn common-templates:build && gulp templatecache:build --gulpfile projects/marvel/gulpfile.js && ng build --project marvel",
    "marvel:watch": "yarn common-templates:watch & gulp templatecache:watch --gulpfile projects/marvel/gulpfile.js & ng build --project marvel --watch",
    "serve": "browser-sync start --config ./bs-config.js",
    "common-templates:build": "gulp templatecache:build --gulpfile projects/common/gulpfile.js",
    "common-templates:watch": "gulp templatecache:watch --gulpfile projects/common/gulpfile.js",
    "watch": "yarn learner:watch & yarn admin:watch & yarn marvel:watch"
  },
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "angular": "1.7.9",
    "angular-animate": "1.7.8",
    "angular-aria": "1.7.8",
    "angular-material": "1.1.11",
    "angular-messages": "1.7.8",
    "angular-mocks": "1.7.8",
    "angular-resource": "1.7.8",
    "angular-sanitize": "1.7.8",
    "angular-ui-router": "0.4.2",
    "components-font-awesome": "4.6.3",
    "jquery": "3.3.1",
    "rxjs": "6.4.0",
    "toastr": "2.1.4",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/angular": "^1.6.55",
    "@types/express": "^4.17.0",
    "@types/node": "~8.9.4",
    "browser-sync": "^2.26.7",
    "codelyzer": "^5.0.0",
    "express": "^4.17.1",
    "gulp": "3.9.1",
    "gulp-angular-templatecache": "2.2.0",
    "gulp-htmlmin": "4.0.0",
    "gulp-load-plugins": "1.5.0",
    "gulp-ng-annotate": "2.1.0",
    "gulp-watch": "5.0.0",
    "morgan": "^1.9.1",
    "ts-node": "^8.2.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
